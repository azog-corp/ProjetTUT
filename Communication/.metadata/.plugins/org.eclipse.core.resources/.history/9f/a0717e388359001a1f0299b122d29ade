package test;

import java.io.IOException;
import java.io.PipedReader;

public class Thre extends Thread{
	
	PipedReader red;
@Override
	public void run() {
		System.out.println("Thread");
		try {
			int test = red.read();
			System.out.println(test);
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
